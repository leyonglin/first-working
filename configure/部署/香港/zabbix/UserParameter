UserParameter=Mount.status,df -TH |awk NR!=1 |awk '{print $2}' |sort |uniq |grep -v tmpfs |grep -v ext |grep -v xfs|wc -l


UserParameter=mysql.status[*],echo "show global status where Variable_name='$1';" | /opt/pub/apps/mysql/bin/mysql -uzabbixmonitor -S /opt/dfcp/data/data_16303/mysql.sock |grep $1 |awk '{print $$2}'
UserParameter=mysql.ping,netstat -lntp |grep 16303 |wc -l
UserParameter=mysql.version,/opt/pub/apps/mysql/bin/mysql -V
UserParameter=mysql.slave.status,/opt/pub/apps/mysql/bin/mysql -uzabbixmonitor -S /opt/dfcp/data/data_16303/mysql.sock -e "show slave status\G" |grep -e Slave_IO_Running -e Slave_SQL_Running:|grep Yes | wc -l


UserParameter=nginx.ping, ps -ef | grep nginx\:\ master | grep -v grep | wc -l
UserParameter=nginx.active,curl -s http://127.0.0.1:10061/nginx_status |grep 'Active'|awk -F':' '{print $2}'
UserParameter=nginx.reading,curl -s http://127.0.0.1:10061/nginx_status|grep 'Reading'|awk -F':' '{print $2}'|awk '{print $1}'
UserParameter=nginx.writing,curl -s http://127.0.0.1:10061/nginx_status|grep 'Reading'|awk -F':' '{print $3}'|awk '{print $1}'
UserParameter=nginx.waiting,curl -s http://127.0.0.1:10061/nginx_status|grep 'Reading'|awk -F':' '{print $4}'
UserParameter=nginx.accepts,curl -s http://127.0.0.1:10061/nginx_status|awk NR==3 |awk '{print $1}'
UserParameter=nginx.handled,curl -s http://127.0.0.1:10061/nginx_status|awk NR==3 |awk '{print $2}'
UserParameter=nginx.requests,curl -s http://127.0.0.1:10061/nginx_status|awk NR==3 |awk '{print $3}'


UserParameter=redis_000,ps -ef | grep redis-server\ 0.0.0.0| grep -v grep | wc -l


UserParameter=tomcat_status[*],netstat -lnt |grep $1 |wc -l
